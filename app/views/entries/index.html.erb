<div class="page-header">
  <div class="logo">
    <h3><span><i class="fa fa-plus-circle"></i></span>Diabee</h3>
  </div><!-- .logo -->
</div>
 <%= link_to 'Add Result', new_entry_path, class: "btn btn-default add" %>
    <% @entries.each do |entry| %>
   
  <div class="entry">

      <div class="container-inner">

        <h3><%= entry.name %></h3>
 
       <p><%= entry.content %></p>

       <div class="meta el">

        <span><i class="fa fa-clock-o"></i><%= link_to time_ago_in_words(entry.created_at), entry %></span>

        <%= link_to entry, method: :delete, data: { confirm: 'Are you sure?' } do %>
          <i class="fa fa-trash-o"></i>

        <% end %>

       </div><!-- .meta -->

     </div><!-- .container-inner -->

  </div><!-- .entry -->

<% end %>

